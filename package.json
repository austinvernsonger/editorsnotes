{
  "dependencies": {
    "browserify": "^3.32.1",
    "underscore": "^1.6.0",
    "backbone": "^1.1.2",
    "browserify-shim": "^3.3.1",
    "node-underscorify": "0.0.7",
    "less": "1.3.0"
  },
  "devDependencies": {
    "mocha": "^1.18.0",
    "testling": "^1.6.1"
  },
  "testling": {
    "harness": "mocha",
    "files": "editorsnotes_app/js/test/*.js",
    "browsers": [
      "firefox/latest"
    ]
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "node-underscorify"
    ]
  },
  "browserify-shim": {
    "jquery": "global:$",
    "citeproc": "global:CSL",
    "wysihtml5": "global:wysihtml5",
    "seadragon": "global:seadragon"
  },
  "scripts": {
    "test": "mocha editorsnotes_app/js/test/*.js"
  }
}
